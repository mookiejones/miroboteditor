<Window x:Class="CSVE6POS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:CSVE6POS"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel>
        <Menu IsMainMenu="true" DockPanel.Dock="Top" Height="25" VerticalAlignment="Top">
            <MenuItem Header="_File" Click="MenuItem_Click_1">
                <MenuItem Header="_Open File"/>
            </MenuItem>
        </Menu>
        <Grid x:Name="grid">
            <Grid.DataContext>
                <local:E6Reader/>
            </Grid.DataContext>
            <DataGrid ItemsSource="{Binding Positions}">

            </DataGrid>

        </Grid>
    </DockPanel>
</Window>
