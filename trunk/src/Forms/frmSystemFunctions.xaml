<Window x:Class="miRobotEditor.Forms.FrmSystemFunctions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="frmSystemFunctions" Height="169" Width="201" ResizeMode="NoResize">
    <Grid>
        <CheckBox Name="ckPrograms" Content="Programs" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="ckFunctions" Content="Functions" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="ckStructures" Content="Structures" HorizontalAlignment="Left" Margin="10,52,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="ckVariables" Content="Variables" HorizontalAlignment="Left" Margin="10,73,0,0" VerticalAlignment="Top"/>
        <Button Content="_Cancel" HorizontalAlignment="Left" Margin="10,102,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="_Ok" HorizontalAlignment="Left" Margin="100,102,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
