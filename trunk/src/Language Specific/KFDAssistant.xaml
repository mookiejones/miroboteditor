<Window x:Class="miRobotEditor.KFDAssistant"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="KFD Assistent"  Height="423" Width="353">
	<Grid Background="Cyan">
		<Menu>
		<MenuItem Header="_Datei" />
		</Menu>
		<StackPanel Orientation="Vertical">
		<ToolBar x:Name="ToolBar1">
			<Button Content="Static"/>
			<Button Content="Free"/>
			<Button Content="Name"/>
			<Button Content="Number"/>
			<Button Content="Real"/>
			<Button Content="List"/>
			<Separator/>
		</ToolBar>
		<ToolBar x:Name="ToolBar2">
			<Button Content="_Neu"/>
			<Button Content="O_ffnen"/>
			<Button Content="_Speichern"/>
			<Separator/>
			<ComboBox>
				<ComboBoxItem Content="MyRollerTP"/>
				<ComboBoxItem Content="MyTech"/>
			</ComboBox>
			<ComboBox>
				<ComboBoxItem Content="On"/>
				<ComboBoxItem Content="Off"/>
			</ComboBox>			<Separator/>
		</ToolBar>
		</StackPanel>
	</Grid>
</Window>