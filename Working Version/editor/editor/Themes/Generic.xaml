        <ResourceDictionary Source="/Themes/MainSkin.xaml" />
        <ResourceDictionary Source="/Themes/Templates.Layout.xaml" />
        <ResourceDictionary Source="/Themes/Templates.StatusBar.xaml" />
        <ResourceDictionary Source="/Themes/Templates.Kuka.xaml" />
